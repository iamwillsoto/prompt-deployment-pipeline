{
  "Comment": "Pixel Prompts pipeline: render -> invoke bedrock -> publish",
  "StartAt": "Render",
  "States": {
    "Render": {
      "Type": "Task",
      "Resource": "${render_arn}",
      "Next": "InvokeBedrock"
    },
    "InvokeBedrock": {
      "Type": "Task",
      "Resource": "${invoke_arn}",
      "Next": "Publish"
    },
    "Publish": {
      "Type": "Task",
      "Resource": "${publish_arn}",
      "End": true
    }
  }
}
